        Label:
            text: 'While many banks have come and gone,'
            font_size: 20
            pos_hint: {'center_x': 0.5, 'center_y': 0.13}
            color: 0.31, 0.32, 0.34, 1
    BoxLayout:
        Label:
            text: 'we successfully foster [i][b]Relationships for Generations[/b][/i].'
