        icon_right: 'key-variant'
        width:270

    MDTextField:
        id:login_password
        helper_text:'Enter Password'
        helper_text_mode: "persistent"
        required:True
        password: True
        pos_hint:{'center_x':0.5,'center_y':0.55}
        size_hint_x:None
        width:270

    MDIconButton:
        icon: "eye-off"
        ripple_scale: .5
        pos_hint: {"center_y": .55, "center_x":0.65}
